package com.bean;

import java.io.Serializable;

/**
 * Created by hzm on 2014/8/29.
 */
public class HtmlReplaceForBean implements Serializable {
    private String name;
    private String express;
    private String content;

    public HtmlReplaceForBean() {
    }

    public HtmlReplaceForBean(String express, String content) {
        this.express = express;
        this.content = content;
    }

    public HtmlReplaceForBean(String name, String express, String content) {
        this.name = name;
        this.express = express;
        this.content = content;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public String getExpress() {
        return express;
    }

    public void setExpress(String express) {
        this.express = express;
    }
}
