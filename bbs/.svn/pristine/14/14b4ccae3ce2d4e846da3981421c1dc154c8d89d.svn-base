function pageClick(action,type){
	var curpage=parseInt($("#curPage"+type).val());
	var pageSize=parseInt($("#pageSize"+type).val());
	var itemCount=parseInt($("#itemCount"+type).val());
	if(action=="minus"){
		curpage=curpage-1;
	} else if(action=="add"){
		curpage=curpage+1;
	} else {
		curpage=action;
	}
	$("#curPage"+type).val(curpage);
	var Hhtml = MyPager.printPage(curpage,pageSize,itemCount,{type:type});
	$("#divPagePanel"+type).html(Hhtml);
	$(".tiaozhuan input").attr("value",curpage);
}
